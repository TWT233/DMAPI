<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <!-- Common Properties-->
    <PropertyGroup>
        <!-- enable line numbers in stack traces (needs to embedded for Linux/macOS: https://github.com/dotnet/runtime/issues/39987) -->
        <DebugSymbols>true</DebugSymbols>
        <DebugType>embedded</DebugType>
    </PropertyGroup>

    <Import Project="reference-dlls.targets" Condition="'$(IfndefReferenceDlls)' == ''"/>
    <Import Project="deploy-build.targets" Condition="'$(IfndefDeployBuild)' == ''"/>

</Project>
